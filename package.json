{
  "name": "vuejs-masterclass-2024",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "supabase:init": "supabase init",
    "supabase:login": "supabase login",
    "supabase:link": "supabase link --project-ref qrmwtiuvcxryqeckuvmb",
    "supabase:types": "npx supabase gen types --lang=typescript --project-id qrmwtiuvcxryqeckuvmb --schema public > database/types.ts",
    "db:migrate:new": "supabase migration new",
    "db:reset": "supabase db reset --linked",
    "db:seed": "node --env-file=.env database/seed.js",
    "db:refresh": "supabase db reset --linked && node --env-file=.env database/seed.js && npx supabase gen types --lang=typescript --project-id qrmwtiuvcxryqeckuvmb --schema public > database/types.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.46.2",
