    "clsx": "^2.1.1",
    "iconify-icon": "^2.1.0",
    "lucide-vue-next": "^0.462.0",
    "pinia": "^2.2.7",
    "radix-vue": "^1.9.10",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
